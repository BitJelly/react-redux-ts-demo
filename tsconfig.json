{
  "compileOnSave": false,
  "buildOnSave": false,
  "lib": "es2017",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "removeComments": false,
    "sourceMap": false,
    "inlineSourceMap": true,
    "noEmitHelpers": false
  },
  "exclude": [
    "node_modules",
    "public"
  ]
}
